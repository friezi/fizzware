TARGET = focal

MAIN_MODULE = main
LOCAL_MODULES = linescanner filescanner
EXTERN_MODULES =
EM_PATH =

CLASSLIBRARY_PATH = ../classes
CLASSLIBRARY = fztooltempl
CLASS_MODULES = mathexpression exception cmdlparser
CM_PATH = $(CLASSLIBRARY_PATH)

IMPORTANT_HEADERS = $(CM_PATH)/datastructures

ifeq ($(UNIX),true)
	READLINEDIR = /usr/gnu/include/readline
	READLINELIBS = readline curses
else
	READLINEDIR = /usr/include/readline
	READLINELIBS = readline
endif

LIBRARY_INCLUDE_PATHS = $(READLINEDIR)
LIBRARIES = $(READLINELIBS)
LIBRARY_PATHS = /usr/lib

